<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>Pickemchief</servlet-name>
		<servlet-class>com.pickemchief.PickemchiefServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Pickemchief</servlet-name>
		<url-pattern>/pickemchief</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>creategame</servlet-name>
		<servlet-class>com.pickemchief.CreateGameServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>creategame</servlet-name>
		<url-pattern>/creategame</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>gamescrape</servlet-name>
		<servlet-class>com.pickemchief.GameScrapeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>gamescrape</servlet-name>
		<url-pattern>/gamescrape</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>savepreseasonpick</servlet-name>
		<servlet-class>com.pickemchief.SavePreseasonPickServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>savepreseasonpick</servlet-name>
		<url-pattern>/savepreseasonpick</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>savepick</servlet-name>
		<servlet-class>com.pickemchief.SavePickServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>savepick</servlet-name>
		<url-pattern>/savepick</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.pickemchief.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>createplayer</servlet-name>
		<servlet-class>com.pickemchief.CreatePlayerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>createplayer</servlet-name>
		<url-pattern>/createplayer</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>getusername</servlet-name>
		<servlet-class>com.pickemchief.GetUsernameServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getusername</servlet-name>
		<url-pattern>/getusername</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>getpicksfromdate</servlet-name>
		<servlet-class>com.pickemchief.GetPicksFromDateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getpicksfromdate</servlet-name>
		<url-pattern>/getpicksfromdate</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>viewalltheirpicks</servlet-name>
		<servlet-class>com.pickemchief.ViewAllTheirPicksServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>viewalltheirpicks</servlet-name>
		<url-pattern>/viewalltheirpicks</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value/>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
</web-app>